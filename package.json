{
  "name": "aviation-maintenance-website",
  "version": "1.0.0",
  "main": ".eleventy.js",
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/css/input.css -o ./src/css/output.css --watch",
    "build:eleventy": "npx eleventy",
    "build": "npm run build:css:prod && npm run build:eleventy",
    "build:css:prod": "npx tailwindcss -i ./src/css/input.css -o ./src/css/output.css --minify",
    "start": "npm run build:css & npx eleventy --serve --port 8080",
    "dev": "concurrently \"npm run build:css\" \"npx eleventy --serve --port 8080\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "playwright": "^1.54.2",
    "website-scraper": "^5.3.1",
    "wrangler": "^4.26.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "tailwindcss": "^3.4.17"
  }
}
